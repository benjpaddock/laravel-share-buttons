{
    "name": "kudashevs/laravel-share-buttons",
    "description": "A laravel share buttons package.",
    "keywords": [
        "laravel",
        "share",
        "share buttons",
        "social links"
    ],
    "homepage": "https://github.com/kudashevs/laravel-share-buttons",
    "license": "MIT",
    "authors": [
        {
            "name": "Joren Van Hocht",
            "email": "joren@codeswitch.be",
            "homepage": "https://codeswitch.be",
            "role": "Creator"
        },
        {
            "name": "Sergey Kudashev",
            "email": "kudashevs@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "~6.7"
    },
    "autoload": {
        "psr-4": {
            "ShareButtons\\Share\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShareButtons\\Share\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShareButtons\\Share\\Providers\\ShareButtonsServiceProvider"
            ],
            "aliases": {
                "Share": "ShareButtons\\Share\\ShareFacade"
            }
        }
    }
}
